#08
# return the string true if num2 is greater than num1,
# otherwise return the string false. If the parameter values are equal to each other then return the string -1.

def check_nums(num1, num2)
  return "-1" if num1 == num2
  num2 > num1 ? true : false
end

check_nums(1,2)
check_nums(1,1)
check_nums(0,0)
check_nums(3,2)
